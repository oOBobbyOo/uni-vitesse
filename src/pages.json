{
  "pages": [
    {
      "path": "pages/index/index",
      "type": "home",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "首页"
      },
      "layout": "default"
    },
    {
      "path": "pages/webview",
      "type": "page"
    },
    {
      "path": "pages/about/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "关于"
      },
      "layout": "default"
    },
    {
      "path": "pages/demo/index",
      "type": "page",
      "style": {
        "navigationBarTitleText": "案例"
      },
      "layout": "demo"
    },
    {
      "path": "pages/hi/index",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "Hi"
      },
      "layout": "default"
    },
    {
      "path": "pages/demo/base/custom-navbar",
      "type": "page",
      "layout": "demo",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "自定义导航栏"
      }
    },
    {
      "path": "pages/demo/base/mp-weixin-share",
      "type": "page",
      "layout": "demo",
      "style": {
        "navigationBarTitleText": "微信分享"
      }
    },
    {
      "path": "pages/demo/comp/ui-swiper-dot",
      "type": "page",
      "layout": "comp",
      "style": {
        "navigationBarTitleText": "SwiperDot 轮播图指示点"
      }
    }
  ],
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "uni-vitesse",
    "navigationBarBackgroundColor": "@navBgColor",
    "navigationBarTextStyle": "@navTxtStyle",
    "backgroundColor": "@bgColor",
    "backgroundTextStyle": "@bgTxtStyle",
    "backgroundColorTop": "@bgColorTop",
    "backgroundColorBottom": "@bgColorBottom"
  },
  "tabBar": {
    "color": "@tabFontColor",
    "selectedColor": "@tabSelectedColor",
    "backgroundColor": "@tabBgColor",
    "borderStyle": "@tabBorderStyle",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "22px",
    "spacing": "3px",
    "list": [
      {
        "iconPath": "@iconPath1",
        "selectedIconPath": "@selectedIconPath1",
        "pagePath": "pages/index/index",
        "text": "首页"
      },
      {
        "iconPath": "@iconPath2",
        "selectedIconPath": "@selectedIconPath2",
        "pagePath": "pages/demo/index",
        "text": "案例"
      },
      {
        "iconPath": "@iconPath3",
        "selectedIconPath": "@selectedIconPath3",
        "pagePath": "pages/about/index",
        "text": "关于"
      }
    ]
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "subPackages": [
    {
      "root": "pages-sub",
      "pages": [
        {
          "path": "demo/index",
          "type": "page",
          "layout": "demo",
          "style": {
            "navigationBarTitleText": "分包页面"
          }
        }
      ]
    }
  ]
}
